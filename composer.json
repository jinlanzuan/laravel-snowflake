{
    "name": "mucts/laravel-snowflake",
    "description": "雪花算法：分布式环境，生成全局唯一的订单号",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "^7.4",
        "ext-openssl": "^7.4",
        "ext-gmp": "^7.4",
        "ext-bcmath": "^7.4",
        "laravel/framework": "^7.0"
    },
    "keywords": ["snowflake", "laravel-snowflake", "laravel"],
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "authors": [
        {
            "name": "herry",
            "email": "yuandeng@aliyun.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MuCTS\\LaravelSnowflake\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuCTS\\LaravelSnowflake\\Providers\\SnowflakeServiceProvider"
            ],
            "aliases": {
                "Snowflake": "MuCTS\\LaravelSnowflake\\Facades\\Snowflake"
            }
        }
    }
}
